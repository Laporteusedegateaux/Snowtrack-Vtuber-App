; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SnowTrack-OpenSource"
config/version="1.0.2"
run/main_scene="res://PNGTuber/PNGTuber.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/image="res://header.png"
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://Common/Settings/default_bus_layout.tres"
driver/enable_input=true

[autoload]

SaveData="*res://Common/Scripts/save_data.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/transparent=true
window/size/window_width_override=1280
window/size/window_height_override=720
window/subwindows/embed_subwindows=false
window/stretch/aspect="keep_width"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="SnowTrack"

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")

[filesystem]

import/fbx/enabled=false

